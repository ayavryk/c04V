{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "indent": [
      true,
      "spaces"
    ],
    "jsx-no-lambda": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "max-classes-per-file": [true, 5],
    "no-var-requires": false,
    "ordered-imports": false,
    "prefer-for-of": false,
    "arrow-parens": false,
    "member-ordering": [
      false
    ],
    "object-literal-sort-keys": false,
    "no-shadowed-variable": false,
    "no-console": [
      false
    ],
    "jsx-alignment": [
      false
    ]
  }
}
